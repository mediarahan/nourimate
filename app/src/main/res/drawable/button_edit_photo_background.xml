<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Shadow Layer -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#E5D8B1" />
            <padding
                android:bottom="3dp"/>
        </shape>
    </item>
